prompt = no
default_md = sha256
x509_extensions = v3_req
distinguished_name = dn

[dn]
C = GR
ST = Attica
L = Athens
O = k8s
emailAddress = -your-email-address-
CN = k8s.sm0ke.local

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = k8s.sm0ke.local
